{
	"manifest_version": 2,
	"default_locale": "zh_CN",
	"name": "__MSG_ext_name__",
	"description": "__MSG_ext_description__",
	"offline_enabled": true,
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "3.0.0",
	"permissions": [ "webRequest", "webRequestBlocking", "clipboardWrite", "tabs", "http://*/", "https://*/", "contextMenus" ],
	"content_scripts": [ {
		"js": [ "static/js/content.js" ],
		"matches": [ "*://*/*" ],
		"run_at": "document_end"
	} ],
	"icons": {
		"16": "static/images/icons/icon-16.png",
		"48": "static/images/icons/icon-48.png",
		"128": "static/images/icons/icon-128.png"
	},
	"background": {
		"page": "background.html"
	},
	"web_accessible_resources": [ "static/css/json.css", "static/lib/zepto.min.js", "static/js/app.js"]
}
